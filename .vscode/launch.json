{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Blazor Server (dotnet watch)",
      "type": "coreclr",
      "request": "launch",
      "program": "/usr/bin/dotnet",
      "args": [
        "watch",
        "--project",
        "${workspaceFolder}/CommunityEventManagementSystem.App"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
